module top_module( 
    input [254:0] in,
    output [7:0] out );

    wire [7:0] count;
    always @*
        begin
            count = 8'h00;
            for(int i = 0; i < $bits(in) ; i++)
                begin
                    if(in[i] == 1'b1)
                        count = count + 1'b1;
                    else
                        count = count ;
                end
        end
    assign out = count;
endmodule
