module top_module (
    input a, b, c, d, e,
    output [24:0] out
);

    wire [4:0] x = {a, b, c, d, e};

    genvar i, j;
    generate
        for (i = 0; i < 5; i = i + 1) begin: row
            for (j = 0; j < 5; j = j + 1) begin: col
                assign out[5*i + j] = ~(x[i] ^ x[j]);  // XNOR
            end
        end
    endgenerate

endmodule
